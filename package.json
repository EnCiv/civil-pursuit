{
  "name": "civil-pursuit",
  "version": "1.0.1",
  "description": "Civil Pursuit - so millions of people can deliberate productively online",
  "main": "index.js",
  "scripts": {
    "test": "babel app --out-dir dist && redtea dist/test/migrate dist/test/run --fork",
    "syntest": "node dist/bin/test",
    "start": "node dist/server/start.js",
    "magic-start": "npm install && npm start",
    "postinstall": "bash post-install.sh",
    "browserify": "bash bin/bundle.sh",
    "watchify": "bash bin/watchify.sh",
    "transpile": "babel app --ignore app/test --out-dir dist --source-maps",
    "hot-transpile": "babel --watch app --ignore app/test --out-dir dist/ --source-maps",
    "less": "lessc assets/less/index.less assets/css/index.css && lessc assets/less/training.less assets/css/training.css",
    "watch-less": "npm run less && node dist/bin/watch-less",
    "min-css": "bash bin/min-css.sh",
    "uglify": "uglifyjs assets/js/main.js -o assets/js/main.min.js",
    "uglify-assets": "bash bin/uglify-assets.sh",
    "deploy": "bash bin/build.sh",
    "migrate": "node dist/bin/migrate",
    "reset": "node dist/bin/reset && node dist/bin/migrate",
    "clean": "node dist/bin/drop-sandbox-dbs.js",
    "packbuild": "webpack --config webpack-prod.config.js",
    "packstart": "npm run packbuild && node dist/server/start.js",
    "client": "webpack serve --config webpack-dev.config.js",
    "server": "nodemon --inspect dist/server/start.js",
    "dev": "echo \"these delays need to be kept up to date\" && concurrently -k \"npm run hot-transpile\" \"npm run watch-less\" \"sleep 45 && npm run server\" \"sleep 55 && npm run client\"",
    "devprod": "echo \"these delays need to be kept up to date\" && concurrently -k \"npm run hot-transpile\" \"npm run watch-less\" \"sleep 45 && nodemon dist/server/start.js\" \"sleep 55 && webpack --config webpack-prod.config.js --watch\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": {
    "email": "davidfridley@enciv.org",
    "name": "David Fridley"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
