{
  "name": "synapp",
  "version": "1.0.0",
  "description": "Synapp",
  "main": "index.js",
  "scripts": {
    "test": "redtea dist/test/migrate dist/test/run --fork",
    "syntest": "node dist/bin/test",
    "start": "node --debug dist/bin/start.js",
    "magic-start": "npm install && npm start",
    "postinstall": "bash post-install.sh",
    "build": "bash bin/bundle.sh",
    "watchify": "bash bin/watchify.sh",
    "transpile": "babel --out-dir dist app",
    "hot-transpile": "babel --watch --out-dir dist/ app",
    "less": "lessc assets/less/index.less assets/css/index.css && lessc assets/less/training.less assets/css/training.css",
    "watch-less": "npm run less && node dist/bin/watch-less",
    "min-css": "bash bin/min-css.sh",
    "uglify": "uglifyjs assets/js/main.js -o assets/js/main.min.js",
    "uglify-assets": "bash bin/uglify-assets.sh",
    "deploy": "bash bin/build.sh",
    "migrate": "node dist/bin/migrate",
    "reset": "node dist/bin/reset && node dist/bin/migrate",
    "clean": "node dist/bin/drop-sandbox-dbs.js"
  },
  "author": {
    "email": "david@synaccord.com",
    "name": "David Fridley"
  },
  "license": "BSD-2-Clause",
  "engines": {
    "node": "~6.11.1",
    "npm": "5.x.x"
  },
  "dependencies": {
    "autosize": "^3.0.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.18.2",
    "bower": "~1.3.12",
    "browserify": "~14.3.0",
    "bson-objectid": "^1.2.3",
    "classnames": "^2.2.5",
    "clean-css": "^4.1.9",
    "cloudinary": "^1.9.0",
    "color": "^1.0.1",
    "colors": "~1.0.3",
    "cookie-parser": "~1.3.3",
    "create-react-ref": "^0.1.0",
    "device": "^0.3.8",
    "express": "^4.16.2",
    "express-session": "~1.10.1",
    "firefox-profile": "^0.3.11",
    "immutability-helper": "^2.4.0",
    "is-url": "^1.2.4",
    "less": "^2.7.3",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "log4js-extend": "^0.1.2",
    "log4js-node-mongodb": "^2.2.1",
    "marked": "^0.3.3",
    "moment": "^2.19.1",
    "mungo": "git+https://github.com/Synaccord/mungo.git#v2.3.3.6",
    "nodemailer": "~1.3.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "passport-twitter": "^1.0.3",
    "postcode-validator": "^1.2.0",
    "promise-sequencer": "^2.0.1",
    "react": "^15.6.2",
    "react-action-state-path": "github:ddfridley/react-action-state-path#actionfilters",
    "react-dom": "^15.6.2",
    "react-flip-move": "^2.9.17",
    "react-jss": "^8.6.0",
    "react-proactive-accordion": "github:ddfridley/react-proactive-accordion#master",
    "redtea": "^3.4.1",
    "shallowequal": "^0.2.2",
    "should": "^7.1.0",
